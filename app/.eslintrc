{
  "env": {
    "browser": true,
    "node": true,
  },
  "globals": {
      "_": true,
      "$": true,
      "App": true,
      "Backbone": true,
      "Backgrid": true,
      "d3": true,
      "io": true,
      "nv": true,
      "window": true
  },
  "rules": {
    "camelcase": 0,
    "comma-spacing": 0,
    "quotes": [0, "single"],
    "new-cap": 0,
    "no-extend-native": 0,
    "no-multi-spaces": 0,
    "no-undef": 2,
    "no-unused-vars": 2,
    "space-infix-ops": 0,
    "strict": 0
  }
}
